{
    "manifest_version": 3,
    "name": "ChatGPT HoverTime",
    "version": "1.0.0",
    "description": "Timestamps for your message prompts (:",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://chatgpt.com/*",
      "https://chat.openai.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://chatgpt.com/*",
          "https://chat.openai.com/*"
        ],
        "js": ["content.js"],
        "css": ["styles.css"],
        "run_at": "document_end"
      }
    ]
  }
  